scalar Long

type Query {
    myMemberInfo: Member!
    memberDuplicatedByEmail(email: String!): Boolean!
    memberDuplicatedByNickname(nickname: String!): Boolean!
}

type Mutation {
    signUp(input: SignUpDto!): Member!
}

type Member {
    id: ID
    email: String
    name: String
    nickname: String
    grade: Int
    major: String
    role: String
}

input SignUpDto {
    email: String!
    password: String!
    name: String!
    nickname: String!
    grade: Int!
    major: String!
}
